    padding: 4px;
    padding-left: 16px;
    padding-right: 16px;
    background-color: rgb(68, 68, 68);
}
QMenu::indicator {
    padding: 6px;
    margin: -1px;
	width: 13px;
	height: 13px;
    background-color: rgb(57, 57, 57);
}
